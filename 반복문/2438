import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
	
		
		Scanner sc = new Scanner(System.in);
	
		int num = sc.nextInt();
		
		for(int i=0; i<num; i++) {
			for(int y=0; y<i+1; y++) {
				System.out.print("*");
			}
			System.out.println();
		}
	
	}
}
