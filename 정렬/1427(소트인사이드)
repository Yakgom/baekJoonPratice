import java.io.BufferedReader;
import java.io.BufferedWriter;
import java.io.IOException;
import java.io.InputStreamReader;
import java.io.OutputStreamWriter;


public class Main {
	public static void main(String[] args) throws NumberFormatException, IOException {
		BufferedReader br = new BufferedReader(new InputStreamReader(System.in));
		BufferedWriter bw = new BufferedWriter(new OutputStreamWriter(System.out));
		
		//StringTokenizer st = new StringTokenizer(br.readLine());
		
		int[] arr = new int[10]; // 숫자의 값은 0 ~ 9 
	
		

		
		String N = br.readLine();   // 숫자가 아닌 문자열로 받음
		for(int i=0; i<N.length(); i++) {  // 문자열의 길이만큼 반복함
			arr[N.charAt(i)-48]++;  // 문자열 char형태로 하나씩 자르면서 -48을 하면 만약 0001 이라면 0번째 인덱스의 값이 3이 쌓이고 1번쨰 인덱스의 수가 1이 올라감
		}
		
		
		for(int i=arr.length-1; i>=0; i--) { // 높은숫자부터 정렬하는것임으로 배열을 뒤에부터 접근하면서 카운팅 정렬해줌 
			while(arr[i] > 0) {
				bw.write(String.valueOf(i)); 
				arr[i]--;
			}
			
		}
		
		
		
		
		
		bw.flush();
		bw.close();
		br.close();
	}
}
